<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="516.0" minWidth="700.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="status.feeStatusViewController">
   <children>
      <Label layoutX="292.0" layoutY="14.0" text="Fee Status">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label layoutX="25.0" layoutY="104.0" text="Technologies">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboTech" editable="true" layoutX="141.0" layoutY="105.0" prefWidth="150.0" />
      
      <Button layoutX="590.0" layoutY="103.0" mnemonicParsing="false" onAction="#doShowStatus" text="Show Status">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <TableView fx:id="tblView" layoutX="25.0" layoutY="183.0" prefHeight="309.0" prefWidth="652.0" />
      <RadioButton fx:id="radFullyPaid" layoutX="321.0" layoutY="104.0" mnemonicParsing="false" onAction="#doChangeFull" text="Fully Paid">
         <font>
            <Font size="18.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="radBal" layoutX="458.0" layoutY="104.0" mnemonicParsing="false" onAction="#doChangeBal" text="Balance">
         <font>
            <Font size="18.0" />
         </font>
      </RadioButton>
   </children>
</AnchorPane>
