<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="500.0" minWidth="700.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="batchManager.batchManagerViewController">
   <children>
      <Button layoutX="240.0" layoutY="26.0" mnemonicParsing="false" onAction="#doFetchAll" text="Fetch And Show Batches">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <TableView fx:id="tableBatch" layoutX="32.0" layoutY="96.0" prefHeight="308.0" prefWidth="634.0" />
      <Button layoutX="182.0" layoutY="428.0" mnemonicParsing="false" onAction="#doExportToExcel" prefHeight="59.0" prefWidth="369.0" text="Export To Excel">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <ImageView fitHeight="40.0" fitWidth="59.0" layoutX="220.0" layoutY="438.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@exl.jpg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
